#include<bits/stdc++.h> 
	using namespace std; 
	class Solution
	{   
	public:
	    int maxSubarrayXOR(int n, int arr[])
	    {  
	        int x=arr[0];
	        int h=0,l=0;
	        int r=0;
	        while(h+1<n)
	        {
	           h++;
	           x=x^arr[h];
	           r=max(r,x);
	           if(r>x)
	           {
	              l++;
	              x=arr[l];
	              if(h!=l)
	                  h--;
	              
	               
	           }
	        }
	        sort(arr,arr+n);
	        if(arr[n-1]>r)
	           return arr[n-1];
	        return r;
	        
	        
	        
	    }
	};
	
	int main() 
	{ 
	    int t;
	    cin>>t;
	    while(t--)
	    {
	        int N, X;
	        cin >> N;
	        int arr[N];
	        for(int i = 0; i < N; i++){
	            cin >> arr[i];
	        }
	
	        Solution ob;
	        cout << ob.maxSubarrayXOR(N, arr) << endl;
	    }
	    return 0; 
	} 
