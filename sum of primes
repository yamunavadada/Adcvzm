#include <iostream>
using namespace std;
long int N=1000;
long int seive[1000];
void genseive()
{
    for(int i=0;i<N;i++)
    {
        seive[i]=1;
    }
    seive[0]=seive[1]=0;
    for(int i=2;i*i<N;i++)
    {
        if(seive[i]==1)
        {
            for(int j=i*i;j<N;j=j+i)
            {
                seive[j]=0; }
        }
    }
}
int main() 
{
    
    int q,l,r;
        cin >> q;
    genseive(); 
    while(q--)
    {   
        int sum=0;  
        cin>>l>>r;                      
        for(int i=l;i<=r;i++)
        {
            if(seive[i]==1)
            {
            sum=sum+i;
            }
        }
        cout<<sum<<endl;
    }
}


