#include <bits/stdc++.h>
	using namespace std;
	class Solution 
	{
	  public:
	    int setBits(int N)
	    {
	        int ct = 0;
	        while(N)
	        {
	            ct = ct + (N&1);
	            N >>= 1;
	        }
	        return ct;
	    }
	};
	int main() {
	    int t;
	    cin >> t;
	    while (t--) {
	        int N;
	        cin >> N;
	
	        Solution ob;
	        int cnt = ob.setBits(N);
	        cout << cnt << endl;
	    }
	    return 0;
	}
